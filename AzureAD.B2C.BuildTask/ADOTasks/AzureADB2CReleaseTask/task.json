{
  "id": "28564eb8-f250-4cd6-b9db-d0d71e050fa4",
  "name": "AzureADB2CReleaseTask",
  "friendlyName": "Release Azure AD(B2C) Policies",
  "description": "Release Azure B2C Custom Policies",
  "author": "Manish Tiwari",
  "helpMarkDown": "",
  "category": "Utility",
  "demands": [],
  "version": {
    "Major": "1",
    "Minor": "0",
    "Patch": "0"
  },
  "visibility": [
    "Release"
  ],
  "icons": {
    "default": "icon.png"
  },
  "minimumAgentVersion": "1.95.0",
  "instanceNameFormat": "AzureB2cCustomPolicies $(message)",
  "inputs": [
    {
      "name": "b2cDomain",
      "type": "string",
      "label": "B2C Domain/ Tenant name",
      "defaultValue": "something",
      "required": true,
      "helpMarkDown": "You can find the tenant name from b2c tenant"
    },
    {
      "name": "clientid",
      "type": "string",
      "label": "Application Id",
      "defaultValue": "00000000-0000-0000-0000-000000000000",
      "required": true,
      "helpMarkDown": "Application Id of the application you have registered in b2c tenant"
    },
    {
      "name": "clientsecret",
      "type": "string",
      "label": "Application Secret",
      "defaultValue": "Applcation Secret",
      "required": true,
      "helpMarkDown": "Application secret of the application you have registered in b2c tenant"
    }
  ],
  "execution": {
    "PowerShell3": {
      "target": "AzureADB2CBuildTask.ps1"
    }
  }
}