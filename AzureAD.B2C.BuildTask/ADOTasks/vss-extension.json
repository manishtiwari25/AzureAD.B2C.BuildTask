{
  "manifestVersion": 1,
  "id": "AzureADB2CBuildTask",
  "version": "2.0.3",
  "name": "Azure AD B2C",
  "description": "Automate Azure B2C custom policy build and release",
  "publisher": "ManishTiwari-Azureb2c",
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "Azure",
    "Azure-b2c",
    "Azure-b2c-policies",
    "trustFrameworkPolicy"
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/manishtiwari25/AzureAD.B2C.BuildTask"
  },
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "contributions": [
    {
      "id": "AzureADB2CBuildTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "AzureADB2CBuildTask"
      }
    },
    {
      "id": "AzureADB2CReleaseTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "AzureADB2CReleaseTask"
      }
    }
  ],
  "files": [
    {
      "path": "AzureADB2CBuildTask"
    },
    {
      "path": "AzureADB2CReleaseTask"
    }
  ],
  "branding": {
    "color": "rgb(34, 34, 34)",
    "theme": "dark"
  },
  "screenshots": [
    {
        "path": "images/build1.png"
    },
    {
        "path": "images/releasepipeline.png"
    },
    {
        "path": "images/release2.png"
    }
  ]
}